{
  "name": "abortable-iterator",
  "version": "2.0.0",
  "description": "Make any iterator or iterable abortable via an AbortSignal",
  "main": "index.js",
  "scripts": {
    "test": "standard && nyc --reporter=lcov --reporter=text ava --verbose"
  },
  "keywords": [
    "async",
    "iterator",
    "abort",
    "abortable",
    "cancel",
    "stop",
    "AbortController",
    "AbortSignal",
    "signal"
  ],
  "author": "Alan Shaw",
  "license": "MIT",
  "devDependencies": {
    "abort-controller": "^3.0.0",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "nyc": "^14.0.0",
    "standard": "^12.0.1"
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "failWithoutAssertions": false
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "get-iterator": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alanshaw/abortable-iterator.git"
  },
  "bugs": {
    "url": "https://github.com/alanshaw/abortable-iterator/issues"
  },
  "homepage": "https://github.com/alanshaw/abortable-iterator#readme"
